                                    {if $finishedTrades[ft].game_result==1}
                                        <td class="in_money">&nbsp;In</td>
                                    {else}
                                        <td class="out_money">&nbsp;Out</td>
                                    </tr>
                                {/if}
                            {/section}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <input type="hidden" name="user_id" id="user_id" value="{$user_id}">
        <div id="static_bottom"></div>
